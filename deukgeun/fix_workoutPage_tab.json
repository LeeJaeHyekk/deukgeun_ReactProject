{
  "page": {
    "name": "WorkoutPage",
    "description": "종합 운동 관리 페이지 - 운동 계획, 진행 상황, 목표, 실시간 세션 트래킹, 분석을 한 페이지에서 확인",
    "mainFile": "WorkoutPage.tsx",
    "architecture": {
      "pattern": "Tab-based Architecture with Zustand State Management",
      "components": "Functional Components with TypeScript",
      "stateManagement": "Zustand Store with DevTools & Persist",
      "styling": "CSS Modules with Tailwind-like utilities",
      "accessibility": "ARIA-compliant tab navigation"
    },
    "tabs": [
      {
        "id": "overview",
        "name": "개요",
        "component": "OverviewTab",
        "description": "운동 통계, 최근 세션, 목표 진행상황을 한눈에 확인",
        "props": ["dashboardData", "isLoading"],
        "actions": ["onPlanClick", "onSessionClick", "onGoalClick"],
        "loadingKey": "overview",
        "subComponents": [
          "StatsSection",
          "RecentSessionsSection",
          "GoalsProgressSection",
          "ChartsSection"
        ]
      },
      {
        "id": "plans",
        "name": "계획",
        "component": "PlansTab",
        "description": "운동 계획 생성, 편집, 실행 관리",
        "props": ["plans", "isLoading"],
        "actions": [
          "onCreatePlan",
          "onEditPlan",
          "onStartSession",
          "onDeletePlan"
        ],
        "loadingKey": "plans",
        "subComponents": ["PlansHeader", "PlansContent", "PlansStats"]
      },
      {
        "id": "sessions",
        "name": "세션",
        "component": "SessionsTab",
        "description": "운동 세션 기록 및 실시간 트래킹",
        "props": ["sessions", "isLoading"],
        "actions": [
          "onCreateSession",
          "onEditSession",
          "onViewSession",
          "onDeleteSession"
        ],
        "loadingKey": "sessions",
        "subComponents": [
          "SessionsHeader",
          "ActiveSessionContainer",
          "SessionsContent",
          "SessionsStats"
        ]
      },
      {
        "id": "goals",
        "name": "목표",
        "component": "GoalsTab",
        "description": "운동 목표 설정 및 진행상황 관리",
        "props": ["goals", "isLoading"],
        "actions": ["onCreateGoal", "onEditGoal", "onDeleteGoal"],
        "loadingKey": "goals",
        "subComponents": [
          "GoalsHeader",
          "GoalsContent",
          "GoalsStats",
          "ActiveGoalsSection",
          "CompletedGoalsSection"
        ]
      },
      {
        "id": "progress",
        "name": "진행상황",
        "component": "ProgressTab",
        "description": "운동 진행상황 차트 및 통계 분석",
        "props": ["sessions", "isLoading"],
        "actions": ["onViewSession"],
        "loadingKey": "progress",
        "subComponents": [
          "ProgressHeader",
          "ProgressContent",
          "ProgressStats",
          "ProgressCharts"
        ]
      }
    ],
    "context": {
      "name": "WorkoutStore",
      "provider": "Zustand Store",
      "description": "전역 상태 관리를 위한 Zustand 스토어",
      "state": {
        "activeTab": "overview",
        "plans": [],
        "sessions": [],
        "goals": [],
        "currentPlan": null,
        "currentSession": null,
        "currentGoal": null,
        "dashboardData": null,
        "loading": {
          "plans": { "isLoading": false, "error": null },
          "sessions": { "isLoading": false, "error": null },
          "goals": { "isLoading": false, "error": null },
          "overview": { "isLoading": false, "error": null }
        },
        "modals": {
          "plan": {
            "isOpen": false,
            "mode": "create",
            "exercises": [],
            "confirmedExerciseIndices": []
          },
          "session": {
            "isOpen": false,
            "mode": "create",
            "currentExerciseIndex": 0,
            "currentSetIndex": 0,
            "isTimerRunning": false
          },
          "goal": {
            "isOpen": false,
            "mode": "create"
          }
        },
        "tabStates": {},
        "sharedState": {
          "notifications": [],
          "lastUpdatedPlan": null,
          "lastUpdatedSession": null,
          "lastUpdatedGoal": null
        }
      },
      "actions": [
        "fetchPlans",
        "createPlan",
        "updatePlan",
        "deletePlan",
        "fetchSessions",
        "createSession",
        "updateSession",
        "deleteSession",
        "fetchGoals",
        "createGoal",
        "updateGoal",
        "deleteGoal",
        "setActiveTab",
        "openPlanModal",
        "closePlanModal",
        "openSessionModal",
        "closeSessionModal",
        "openGoalModal",
        "closeGoalModal",
        "updateTabState",
        "resetTabState",
        "updateSharedState",
        "addNotification",
        "removeNotification",
        "clearNotifications",
        "resetStore",
        "clearErrors"
      ]
    },
    "hooks": [
      {
        "name": "useWorkoutStoreData",
        "description": "Zustand store에서 데이터 상태를 가져오는 훅",
        "returns": [
          "plans",
          "sessions",
          "goals",
          "dashboardData",
          "loading",
          "activeTab",
          "modals",
          "tabStates",
          "sharedState"
        ]
      },
      {
        "name": "useWorkoutPlansActions",
        "description": "운동 계획 관련 액션들을 제공하는 훅",
        "returns": ["fetchPlans", "createPlan", "updatePlan", "deletePlan"]
      },
      {
        "name": "useWorkoutSessionsActions",
        "description": "운동 세션 관련 액션들을 제공하는 훅",
        "returns": [
          "fetchSessions",
          "createSession",
          "updateSession",
          "deleteSession"
        ]
      },
      {
        "name": "useWorkoutGoalsActions",
        "description": "운동 목표 관련 액션들을 제공하는 훅",
        "returns": ["fetchGoals", "createGoal", "updateGoal", "deleteGoal"]
      },
      {
        "name": "useWorkoutUI",
        "description": "UI 상태 관리 액션들을 제공하는 훅",
        "returns": [
          "setActiveTab",
          "openPlanModal",
          "closePlanModal",
          "openSessionModal",
          "closeSessionModal",
          "openGoalModal",
          "closeGoalModal",
          "updateTabState",
          "resetTabState"
        ]
      },
      {
        "name": "useWorkoutInitialization",
        "description": "초기 데이터 로딩을 담당하는 훅",
        "returns": ["initializeWorkoutData"]
      },
      {
        "name": "useSharedState",
        "description": "공유 상태 관리 훅",
        "returns": [
          "updateSharedState",
          "addNotification",
          "removeNotification",
          "clearNotifications"
        ]
      }
    ],
    "tabContent": {
      "component": "TabContent",
      "description": "현재 탭 상태에 따라 적절한 탭 컴포넌트를 렌더링",
      "props": [
        "activeTab",
        "dashboardData",
        "plans",
        "sessions",
        "goals",
        "plansLoading",
        "sessionsLoading",
        "goalsLoading",
        "onPlanClick",
        "onSessionClick",
        "onGoalClick",
        "onCreatePlan",
        "onEditPlan",
        "onStartSession",
        "onCreateSession",
        "onEditSession",
        "onViewSession",
        "onCreateGoal",
        "onEditGoal",
        "onDeletePlan",
        "onDeleteSession",
        "onDeleteGoal"
      ],
      "behavior": [
        "activeTab에 따라 OverviewTab, PlansTab, SessionsTab, GoalsTab, ProgressTab 렌더링",
        "각 탭에 필요한 props 및 액션 연결",
        "로딩 상태에 따라 로딩 화면 표시",
        "조건부 렌더링으로 성능 최적화"
      ]
    },
    "modals": [
      {
        "id": "workoutPlanModal",
        "component": "WorkoutPlanModal",
        "description": "운동 계획 생성/편집 모달",
        "props": [
          "isOpen",
          "mode",
          "plan",
          "exercises",
          "confirmedExerciseIndices",
          "onClose",
          "onSave",
          "onAddExercise",
          "onRemoveExercise",
          "onUpdateExercise",
          "onConfirmExercise",
          "onUnconfirmExercise"
        ]
      },
      {
        "id": "workoutSessionModal",
        "component": "WorkoutSessionModal",
        "description": "운동 세션 생성/편집 모달",
        "props": [
          "isOpen",
          "mode",
          "session",
          "currentExerciseIndex",
          "currentSetIndex",
          "isTimerRunning",
          "onClose",
          "onSave",
          "onStartTimer",
          "onPauseTimer",
          "onCompleteSet"
        ]
      },
      {
        "id": "workoutGoalModal",
        "component": "WorkoutGoalModal",
        "description": "운동 목표 생성/편집 모달",
        "props": ["isOpen", "mode", "goal", "onClose", "onSave"]
      }
    ],
    "stateManagement": {
      "type": "Zustand Store",
      "store": "workoutStore",
      "description": "전역 상태 관리를 위한 Zustand 스토어",
      "initialState": "위 context.state 값 참조",
      "middleware": ["devtools", "persist"],
      "dispatchActions": [
        "fetchPlans",
        "createPlan",
        "updatePlan",
        "deletePlan",
        "fetchSessions",
        "createSession",
        "updateSession",
        "deleteSession",
        "fetchGoals",
        "createGoal",
        "updateGoal",
        "deleteGoal",
        "setActiveTab",
        "openPlanModal",
        "closePlanModal",
        "openSessionModal",
        "closeSessionModal",
        "openGoalModal",
        "closeGoalModal",
        "updateTabState",
        "resetTabState",
        "updateSharedState",
        "addNotification",
        "removeNotification",
        "clearNotifications",
        "resetStore",
        "clearErrors"
      ]
    },
    "integration": {
      "TabContent": "activeTab 상태에 따라 해당 탭 컴포넌트를 렌더링",
      "각 탭 컴포넌트": "props: store state에서 필요한 데이터 전달 / actions: store actions 사용 가능",
      "모달": "Zustand store의 modals 상태로 관리",
      "API 통신": "workoutApi를 통한 서버 통신",
      "타이머": "WorkoutTimerContext와 연동",
      "인증": "AuthContext와 연동하여 사용자별 데이터 관리",
      "기구 데이터": "useMachines 훅을 통한 기구 정보 연동"
    },
    "constants": {
      "TAB_CONFIG": [
        { "key": "overview", "label": "개요", "icon": "📊" },
