{
  "name": "deukgeun",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:frontend": "vite --port 5173",
    "dev:backend": "cd src/backend && npm run dev",
    "dev:backend:wait": "wait-on http://localhost:5173 && npm run dev:backend",
    "dev:all": "concurrently \"npm run dev:frontend\" \"npm run dev:backend:wait\"",
    "check-ports": "node scripts/check-ports.js",
    "build": "vite build",
    "build:type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "preview": "vite preview",
    "test:getGyms": "node --loader ts-node/esm src/script/testGetGyms.ts"
  },
  "dependencies": {
    "@react-three/drei": "^10.1.2",
    "@react-three/fiber": "^9.1.2",
    "@tanstack/react-query": "^5.79.2",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "express": "^5.1.0",
    "framer-motion": "^12.23.9",
    "gsap": "^3.13.0",
    "lucide-react": "^0.525.0",
    "motion": "^12.16.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.4",
    "pretendard": "^1.3.9",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-google-recaptcha": "^3.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.1",
    "three": "^0.177.0",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/express": "^5.0.3",
    "@types/gsap": "^1.20.2",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
