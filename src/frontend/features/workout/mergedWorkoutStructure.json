{
  "workout_journal_flow": {
    "authentication": {
      "description": "사용자가 운동일지 접근 전 로그인 확인",
      "steps": [
        {
          "action": "사용자 /workout-journal 접근",
          "condition": "로그인 여부 확인",
          "next": "페이지 렌더링 or /login 리다이렉트"
        },
        {
          "action": "AuthContext 로딩",
          "condition": "isLoading=true",
          "next": "스피너 표시"
        },
        {
          "action": "인증 실패",
          "next": "로그인 페이지 이동 + 에러 메시지"
        }
      ]
    },
    "overview_tab": {
      "description": "전체 운동 진행 상황 확인",
      "components": ["OverviewTab"],
      "steps": [
        {
          "action": "페이지 진입",
          "next": "Dashboard API 호출 → 통계 데이터 로드"
        },
        {
          "action": "통계 카드 및 위젯 표시",
          "sections": [
            "stats-section",
            "recent-plans-section",
            "recent-sessions-section",
            "goals-progress-section",
            "charts-section"
          ]
        },
        {
          "action": "카드 클릭",
          "next": "해당 탭으로 이동 또는 모달 열기"
        },
        {
          "action": "데이터 없음",
          "next": "운동 계획 생성 안내 메시지"
        }
      ]
    },
    "plans_tab": {
      "description": "운동 계획 생성, 수정, 삭제, 세션 시작",
      "components": ["PlansTab"],
      "steps": [
        {
          "action": "새 계획 만들기 클릭",
          "next": "WorkoutPlanModal 열기 → 필수 입력값 입력"
        },
        {
          "action": "계획 생성",
          "next": "useWorkoutPlans.createPlan() → DB 저장 → 상태 업데이트"
        },
        {
          "action": "계획 카드 표시",
          "features": ["편집", "삭제", "세션 시작"]
        },
        {
          "action": "계획 수정/운동 추가",
          "condition": "진행 중 세션 없음",
          "next": "WorkoutPlanModal 열기 → 수정 가능"
        },
        {
          "action": "세션 시작",
          "next": "WorkoutSessionModal → 세션 생성 → Timer 시작"
        },
        {
          "exception": "세션 진행 중 계획 수정 시",
          "next": "경고 모달 표시"
        }
      ]
    },
    "sessions_tab": {
      "description": "운동 세션 진행 및 세트 인증",
      "components": ["SessionsTab", "GlobalWorkoutTimer"],
      "steps": [
        {
          "action": "세션 생성",
          "state": "in_progress"
        },
        {
          "action": "전역 타이머 시작",
          "next": "실시간 세트 진행 기록"
        },
        {
          "action": "세트 완료",
          "next": "ExerciseSet 생성 → DB 저장 → 목표 진행률 업데이트"
        },
        {
          "action": "일시정지",
          "next": "타이머 일시정지"
        },
        {
          "action": "세션 완료",
          "next": "상태 변경, 총 시간 계산, 레벨 경험치 부여"
        },
        {
          "exception": "인터넷 끊김",
          "next": "로컬 스토리지 임시 저장, 세션 복구"
        },
        {
          "exception": "타이머 오작동",
          "next": "수동 완료 입력 허용"
        }
      ]
    },
    "goals_tab": {
      "description": "목표 생성, 수정, 삭제 및 진행률 확인 (자동 섹션 생성 포함)",
      "components": ["GoalsTab"],
      "steps": [
        {
          "action": "목표 생성",
          "next": "WorkoutGoalModal → DB 저장 → 자동 섹션 생성 → 상태 업데이트"
        },
        {
          "action": "자동 섹션 생성",
          "logic": "목표 타입에 따른 운동 섹션 자동 생성",
          "goal_types": {
            "weight": "근력 운동 섹션 생성",
            "reps": "지구력 운동 섹션 생성",
            "duration": "유산소 운동 섹션 생성",
            "frequency": "전체 운동 계획 생성"
          }
        },
        {
          "action": "세트 완료 시 진행률 업데이트",
          "next": "섹션 진행률 → 목표 진행률 업데이트"
        },
        {
          "action": "목표 달성",
          "state": "isCompleted=true",
          "next": "레벨 경험치 부여, 알림 생성"
        },
        {
          "action": "목표 수정/삭제",
          "condition": "세션 진행 중이면 제한"
        },
        {
          "exception": "목표 삭제 시 진행 중 세션 영향",
          "next": "확인 모달"
        }
      ]
    },
    "progress_tab": {
      "description": "통계 및 히스토리 시각화",
      "components": ["ProgressTab"],
      "steps": [
        {
          "action": "탭 클릭",
          "next": "API 호출 → 차트/캘린더 데이터 로드"
        },
        {
          "action": "데이터 시각화",
          "components": ["ProgressChart", "WorkoutCalendar"],
          "features": ["주간/월간/연간 차트", "시간 범위 선택", "활동 목록"]
        },
        {
          "exception": "데이터 없음",
          "next": "안내 메시지 표시"
        }
      ]
    },
    "modals_and_sync": {
      "description": "모달 연동 및 실시간 데이터 동기화",
      "modals": [
        "WorkoutPlanModal",
        "WorkoutSessionModal",
        "WorkoutGoalModal",
        "WorkoutSectionModal"
      ],
      "steps": [
        {
          "action": "모달 제출",
          "next": "API 호출 → DB 저장 → 상태 업데이트 → UI Refresh"
        },
        {
          "action": "실시간 동기화",
          "methods": ["useEffect", "useCallback"],
          "targets": ["타이머", "목표 진행률", "세션 상태"]
        },
        {
          "exception": "제출 실패",
          "next": "롤백 및 에러 메시지"
        },
        {
          "exception": "동시 편집",
          "next": "경고 메시지, 마지막 입력 우선"
        }
      ]
    },
    "tab_navigation": {
      "description": "탭 간 이동 및 상태 관리",
      "steps": [
        {
          "action": "탭 클릭",
          "next": "activeTab 상태 변경 → 해당 탭 컴포넌트 렌더링"
        },
        {
          "action": "탭별 데이터 로딩",
          "next": "각 탭별 필요한 데이터만 로드 (최적화)"
        },
        {
          "action": "탭 간 데이터 공유",
          "next": "공통 상태를 통해 탭 간 데이터 동기화"
        }
      ]
    }
  },
  "workout_journal_flowchart": {
    "nodes": [
      {
        "id": "auth_check",
        "label": "로그인 상태 확인",
        "type": "decision",
        "condition": "isLoggedIn",
        "true": "main_container",
        "false": "redirect_login"
      },
      {
        "id": "redirect_login",
        "label": "로그인 페이지로 리다이렉트",
        "type": "action"
      },
      {
        "id": "main_container",
        "label": "메인 컨테이너",
        "type": "container",
        "components": ["tab_navigation", "tab_content"]
      },
      {
        "id": "tab_navigation",
        "label": "탭 네비게이션",
        "type": "component",
        "tabs": ["overview", "plans", "sessions", "goals", "progress"]
      },
      {
        "id": "overview_tab",
        "label": "Overview 탭",
        "type": "tab",
        "component": "OverviewTab",
        "actions": ["통계 확인", "최근 활동 확인", "목표 진행률 확인"]
      },
      {
        "id": "plans_tab",
        "label": "Plans 탭",
        "type": "tab",
        "component": "PlansTab",
        "actions": ["새 계획 생성", "계획 수정", "세션 시작"]
      },
      {
        "id": "sessions_tab",
        "label": "Sessions 탭",
        "type": "tab",
        "component": "SessionsTab",
        "actions": ["세션 시작", "세트 완료", "세션 일시정지/완료"]
      },
      {
        "id": "goals_tab",
        "label": "Goals 탭",
        "type": "tab",
        "component": "GoalsTab",
        "actions": ["목표 생성/수정/삭제", "자동 섹션 생성", "진행률 확인"]
      },
      {
        "id": "progress_tab",
        "label": "Progress 탭",
        "type": "tab",
        "component": "ProgressTab",
        "actions": ["차트 및 캘린더 표시", "통계 확인"]
      },
      {
        "id": "create_plan_modal",
        "label": "WorkoutPlanModal",
        "type": "modal",
        "trigger": "새 계획 생성 버튼 클릭",
        "next": "plan_created"
      },
      {
        "id": "plan_created",
        "label": "운동 계획 생성 완료",
        "type": "action",
        "next": "plans_tab_refresh"
      },
      {
        "id": "plans_tab_refresh",
        "label": "Plans 탭 새로고침",
        "type": "action",
        "next": "plans_tab"
      },
      {
        "id": "create_session_modal",
        "label": "WorkoutSessionModal",
        "type": "modal",
        "trigger": "세션 시작 버튼 클릭",
        "next": "session_in_progress"
      },
      {
        "id": "session_in_progress",
        "label": "세션 진행 중",
        "type": "state",
        "actions": ["세트 완료", "타이머 일시정지", "세션 완료"],
        "next": ["update_goal_progress", "sessions_tab_refresh"]
      },
      {
        "id": "update_goal_progress",
        "label": "목표 진행률 업데이트",
        "type": "action",
        "next": ["goal_achieved_check", "sessions_tab_refresh"]
      },
      {
        "id": "goal_achieved_check",
        "label": "목표 달성 감지",
        "type": "decision",
        "condition": "WorkoutGoal.currentValue >= targetValue",
        "true": "reward_exp",
        "false": "sessions_tab_refresh"
      },
      {
        "id": "reward_exp",
        "label": "경험치 부여 및 알림",
        "type": "action",
        "next": "goals_tab_refresh"
      },
      {
        "id": "sessions_tab_refresh",
        "label": "Sessions 탭 새로고침",
        "type": "action",
        "next": "sessions_tab"
      },
      {
        "id": "goals_tab_refresh",
        "label": "Goals 탭 새로고침",
        "type": "action",
        "next": "goals_tab"
      },
      {
        "id": "create_goal_modal",
        "label": "WorkoutGoalModal",
        "type": "modal",
        "trigger": "목표 생성 버튼 클릭",
        "next": "auto_section_generation"
      },
      {
        "id": "auto_section_generation",
        "label": "자동 섹션 생성",
        "type": "action",
        "next": "goal_created"
      },
      {
        "id": "goal_created",
        "label": "목표 생성 완료",
        "type": "action",
        "next": "goals_tab_refresh"
      },
      {
        "id": "offline_or_error",
        "label": "인터넷 끊김 / 제출 실패",
        "type": "exception",
        "actions": ["로컬 저장", "롤백", "재시도"]
      }
    ],
    "edges": [
      { "from": "auth_check", "to": "main_container", "condition": "true" },
      { "from": "auth_check", "to": "redirect_login", "condition": "false" },
      { "from": "main_container", "to": "tab_navigation" },
      {
        "from": "tab_navigation",
        "to": "overview_tab",
        "trigger": "overview_tab_click"
      },
      {
        "from": "tab_navigation",
        "to": "plans_tab",
        "trigger": "plans_tab_click"
      },
      {
        "from": "tab_navigation",
        "to": "sessions_tab",
        "trigger": "sessions_tab_click"
      },
      {
        "from": "tab_navigation",
        "to": "goals_tab",
        "trigger": "goals_tab_click"
      },
      {
        "from": "tab_navigation",
        "to": "progress_tab",
        "trigger": "progress_tab_click"
      },
      {
        "from": "plans_tab",
        "to": "create_plan_modal",
        "trigger": "새 계획 생성 클릭"
      },
      { "from": "create_plan_modal", "to": "plan_created" },
      { "from": "plan_created", "to": "plans_tab_refresh" },
      { "from": "plans_tab_refresh", "to": "plans_tab" },
      {
        "from": "sessions_tab",
        "to": "create_session_modal",
        "trigger": "세션 시작 클릭"
      },
      { "from": "create_session_modal", "to": "session_in_progress" },
      {
        "from": "session_in_progress",
        "to": "update_goal_progress",
        "trigger": "세트 완료"
      },
      { "from": "update_goal_progress", "to": "goal_achieved_check" },
      {
        "from": "goal_achieved_check",
        "to": "reward_exp",
        "condition": "true"
      },
      {
        "from": "goal_achieved_check",
        "to": "sessions_tab_refresh",
        "condition": "false"
      },
      { "from": "reward_exp", "to": "goals_tab_refresh" },
      { "from": "sessions_tab_refresh", "to": "sessions_tab" },
      { "from": "goals_tab_refresh", "to": "goals_tab" },
      {
        "from": "goals_tab",
        "to": "create_goal_modal",
        "trigger": "목표 생성 클릭"
      },
      { "from": "create_goal_modal", "to": "auto_section_generation" },
      { "from": "auto_section_generation", "to": "goal_created" },
      { "from": "goal_created", "to": "goals_tab_refresh" }
    ]
  },
